# API key used to authenticate requests to the backend
API_KEY=your_api_key_here

# Backend API endpoint for air quality data
API_URL=http://localhost:8080/air-quality/create

# Arduino (or sensor device) endpoint to fetch air quality metrics
ARDUINO_URL=http://localhost:8000/get-metrics

# Interval (in seconds) between each data collection cycle
INTERVAL=300

# Local file used to store payloads when sending to API fails
PENDING_FILE=pending_payloads.txt